{
	"name": "momentum-panorama",
	"version": "2.0.0",
	"description": "Scripts for handling Momentum Panorama files",
	"config": {
		"baseDir": "./"
	},
	"scripts": {
		"ts:check": "tsc --noEmit",
		"ts:watch": "tsc --watch --noEmit",
		"format:fix": "prettier --write **/*.{js,ts,scss,css,md}",
		"format:check": "prettier --check **/*.{js,ts,scss,css,md}",
		"lint:fix": "eslint \"**/*.{js,ts}\" --fix",
		"lint:check": "eslint \"**/*.{js,ts}\"",
		"prepare": "husky",
		"pre-commit": "lint-staged",
		"get-learn": "node tools/get-learn.js",
		"get-credits": "tsx tools/get-credits.ts",
		"fetch-web": "bash tools/fetch-web.sh"
	},
	"type": "module",
	"author": "Momentum Team",
	"license": "MIT",
	"devDependencies": {
		"@eslint/js": "^9.22.0",
		"@types/eslint__js": "^9.14.0",
		"@types/papaparse": "^5.3.16",
		"axios": "^1.8.3",
		"eslint": "^9.22.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.3",
		"eslint-plugin-unicorn": "^57.0.0",
		"eslint-plugin-unused-imports": "^4.1.4",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"papaparse": "^5.5.2",
		"prettier": "^3.5.3",
		"prettier-eslint": "^16.3.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.26.1"
	},
	"lint-staged": {
		"**/*.{js,ts,scss,css,md}": [
			"prettier --write"
		],
		"**/*.{js,ts}": "eslint --fix"
	}
}
