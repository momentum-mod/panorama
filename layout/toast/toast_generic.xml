<root>
	<styles>
		<include src="file://{resources}/styles/main.scss" />
	</styles>

	<Panel class="toast toast-generic">
		<Image id="Icon" class="toast-generic__icon" textureheight="64" />
		<Panel class="toast-generic__text-container">
			<Label id="Title" class="toast-generic__title" text="{s:toast_title}" />
			<Label class="toast-generic__message" text="{s:toast_message}" />
		</Panel>
	</Panel>
</root>
