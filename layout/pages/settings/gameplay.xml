<root>
	<styles>
		<include src="file://{resources}/styles/main.scss" />
	</styles>

	<scripts>
		<include type="module" src="file://{scripts}/pages/settings/page.ts" />
	</scripts>

	<Panel class="settings-page">
		<Panel class="settings-page__header">
			<Label class="settings-page__title" text="#Settings_Gameplay" />
			<Panel class="settings-page__header-buttons">
				<!-- Gamemode Dropdown will go here -->
			</Panel>
		</Panel>

		<Panel id="SettingsPageContainer" class="settings-page__container">
			<Panel id="GameplayGeneralSubSection" class="settings-group">
				<Panel class="settings-group__header">
					<Label class="settings-group__title" text="#Settings_Gameplay_General_Title" tags="gameplay" />
					<Panel class="flow-right h-align-right">
						<Button class="button mr-3" onactivate="SettingsPage.showImportExportDialogue('Settings_Gameplay', 'GameplayGeneralSubSection')">
							<Label class="button__text" text="#Settings_ImportExport_ImportExport" />
						</Button>
						<TooltipPanel class="settings-group__reset" tooltip="#Settings_General_Reset">
							<Button class="button" onactivate="SettingsPage.resetSettings('GameplayGeneralSubSection');">
								<Image class="button__icon" src="file://{images}/reset.svg" />
							</Button>
						</TooltipPanel>
					</Panel>
				</Panel>

				<SettingsSlider text="#Settings_Gameplay_YawSpeed" min="0" max="360" convar="cl_yawspeed" tags="yawspeed,turnbind,spin" infomessage="#Settings_Gameplay_YawSpeed_Info" hasdocspage="false" />

				<SettingsEnum text="#Settings_Gameplay_OverlappingKeys" convar="mom_null_binds" infomessage="#Settings_Gameplay_OverlappingKeys_Info" tags="overlapping,nulls">
					<RadioButton group="allowoverlap" text="#Settings_Gameplay_OverlappingKeys_Off" value="0" />
					<RadioButton group="allowoverlap" text="#Settings_Gameplay_OverlappingKeys_On" value="1" />
				</SettingsEnum>

				<SettingsEnum text="#Settings_Gameplay_ReleaseForwardOnJump" convar="mom_release_forward_on_jump" infomessage="#Settings_Gameplay_ReleaseForwardOnJump_Info" tags="jump,forward">
					<RadioButton group="releaseforward" text="#Common_Off" value="0" />
					<RadioButton group="releaseforward" text="#Common_On" value="1" />
				</SettingsEnum>

				<SettingsEnum text="#Settings_Gameplay_DrawViewmodel" convar="r_drawviewmodel" hasdocspage="false" infomessage="#Settings_Gameplay_DrawViewmodel_Info" tags="viewmodel,weaponmodel">
					<RadioButton group="showviewmodel" text="#Common_Off" value="0" />
					<RadioButton group="showviewmodel" text="#Common_On" value="1" />
				</SettingsEnum>

				<ConVarEnabler convar="r_drawviewmodel" class="flow-down">
					<SettingsSlider text="#Settings_Gameplay_ViewmodelFOV" convar="fov_viewmodel" infomessage="#Settings_Gameplay_ViewmodelFOV_Info" tags="viewmodel,weaponmodel,fov" min="45" max="135" />
				</ConVarEnabler>

				<SettingsEnumDropDown text="#Settings_Gameplay_WeaponDesirability" convar="mom_weapon_preference" infomessage="#Settings_Gameplay_WeaponDesirability_Info" tags="weapon,spawn,desirability">
					<Label id="wepdes1" text="#Settings_Gameplay_WeaponDesirability_Required" value="0" />
					<Label id="wepdes2" text="#Settings_Gameplay_WeaponDesirability_Recommended" value="1" />
					<Label id="wepdes3" text="#Settings_Gameplay_WeaponDesirability_All" value="2" />
				</SettingsEnumDropDown>

				<Panel class="settings-group__combo">
					<SettingsSlider text="#Settings_Gameplay_NoclipSpeed" convar="sv_noclipspeed" min="0" max="20" />
					<Panel class="flow-right w-full">
						<SettingsSlider class="w-fill mr-2" text="#Settings_Gameplay_NoclipDuckModifier" min="0" max="5" displayprecision="1" convar="sv_noclipspeed_duck_multiplier" />
						<SettingsSlider class="w-fill ml-2" text="#Settings_Gameplay_NoclipSprintModifier" min="0" max="5" displayprecision="1" convar="sv_noclipspeed_sprint_multiplier" />
					</Panel>
				</Panel>

			</Panel>

			<Panel class="settings-page__spacer" />

			<Panel id="PaintSubSection" class="settings-group">
