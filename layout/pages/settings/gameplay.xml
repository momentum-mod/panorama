						<Button class="button mr-3" onactivate="SettingsPage.showImportExportDialogue('Settings_Paint_Title', 'PaintSubSection')">
							<Label class="button__text" text="#Settings_ImportExport_ImportExport" />
						</Button>
						<TooltipPanel class="settings-group__reset" tooltip="#Settings_General_Reset">
							<Button class="button" onactivate="SettingsPage.resetSettings('PaintSubSection');">
								<Image class="button__icon" src="file://{images}/reset.svg" />
							</Button>
						</TooltipPanel>
					</Panel>
				</Panel>

				<Panel class="flow-right">
					<Panel id="PaintContainer" class="settings__paint">
						<Panel id="PaintBlob" class="settings__paint-blob" />
					</Panel>
					<Panel class="w-fill flow-down">
						<ConVarColorDisplay convar="mom_paint_color" text="#Settings_Paint_Color" oncolorchange="SettingsPage.updatePaintPreview();" tags="color" />

						<SettingsSlider text="#Settings_Paint_Scale" min="0.001" max="1" percentage="true" convar="mom_paint_size" tags="color,size,scale" onvaluechanged="SettingsPage.updatePaintPreview();" />

						<SettingsEnum text="#Settings_Paint_ApplySound" convar="mom_paint_sound_enable" tags="paint,sound,noise">
							<RadioButton group="paintsound" text="#Common_Off" value="0" />
							<RadioButton group="paintsound" text="#Common_On" value="1" />
						</SettingsEnum>

						<SettingsEnum text="#Settings_Paint_LimitToWorld" convar="mom_paint_world_only" infomessage="#Settings_Paint_LimitToWorld_Info" tags="world">
							<RadioButton group="paintlimit" text="#Common_Off" value="0" />
							<RadioButton group="paintlimit" text="#Common_On" value="1" />
						</SettingsEnum>

						<SettingsEnum text="#Settings_Paint_ColorFromNormal" convar="mom_paint_normal_based_color" infomessage="#Settings_Paint_ColorFromNormal_Info" tags="color,angle">
							<RadioButton group="paintanglecolor" text="#Common_Off" value="0" />
							<RadioButton group="paintanglecolor" text="#Common_On" value="1" />
						</SettingsEnum>
					</Panel>
