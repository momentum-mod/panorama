<root>
	<styles>
		<include src="file://{resources}/styles/main.scss" />
	</styles>
	<scripts>
		<include src="file://{resources}/scripts/settings_shared.js" />
	</scripts>

	<Panel class="settings-page">

		<Panel class="settings-page__header">

			<Label class="settings-page__title" text="#Settings_Gameplay_Title" />

			<Panel class="settings-page__header-buttons">
				<!-- Gamemode Dropdown will go here -->
			</Panel>

		</Panel>

		<Panel id="SettingsPageContainer" class="settings-page__container">

			<Panel id="GameplayGeneralSubSection" class="settings-group">

				<Panel class="settings-group__header">

					<Label class="settings-group__title" text="#Settings_Gameplay_General_Title" />

					<Panel class="flow-right horizontal-align-right">

						<Button
							class="button mr-3"
							onactivate="SettingsShared.showImportExportDialogue('Settings_Gameplay', 'GameplayGeneralSubSection')"
						>
							<Label class="button__text" text="#Settings_ImportExport_ImportExport" />
						</Button>

						<TooltipPanel class="settings-group__reset" tooltip="#Settings_General_Reset">
							<Button class="button" onactivate="SettingsShared.resetSettings('GameplayGeneralSubSection');">
								<Image class="button__icon" src="file://{images}/reset.svg" />
							</Button>
						</TooltipPanel>

					</Panel>

				</Panel>

				<ChaosSettingsSlider
					text="#Settings_Gameplay_YawSpeed"
					min="0"
					max="360"
					convar="cl_yawspeed"
					infomessage="#Settings_Gameplay_YawSpeed_Info"
					hasdocspage="false"
				/>



				<ChaosSettingsEnum
					text="#Settings_Gameplay_OverlappingKeys"
					convar="mom_enable_overlapping_keys"
					infomessage="#Settings_Gameplay_OverlappingKeys_Info"
				>
					<RadioButton group="allowoverlap" text="#Settings_Gameplay_OverlappingKeys_Off" value="0" />
					<RadioButton group="allowoverlap" text="#Settings_Gameplay_OverlappingKeys_On" value="1" />
				</ChaosSettingsEnum>

				<ChaosSettingsEnum
					text="#Settings_Gameplay_ReleaseForwardOnJump"
					convar="mom_release_forward_on_jump"
					infomessage="#Settings_Gameplay_ReleaseForwardOnJump_Info"
				>
					<RadioButton group="releaseforward" text="#Common_Off" value="0" />
					<RadioButton group="releaseforward" text="#Common_On" value="1" />
				</ChaosSettingsEnum>

				<ChaosSettingsEnum
					text="#Settings_Gameplay_DrawViewmodel"
					convar="r_drawviewmodel"
					hasdocspage="false"
					infomessage="#Settings_Gameplay_DrawViewmodel_Info"
				>
					<RadioButton group="showviewmodel" text="#Common_Off" value="0" />
					<RadioButton group="showviewmodel" text="#Common_On" value="1" />
				</ChaosSettingsEnum>

				<ChaosSettingsEnumDropDown
					text="#Settings_Gameplay_WeaponDesirability"
					convar="mom_player_weapon_spawn_desirability"
					infomessage="#Settings_Gameplay_WeaponDesirability_Info"
				>
